@Html.Partial("FormFilter")

    <div class="table-responsive">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>@sReportsV2.Resources.TextLanguage.Title</th>
                    <th>@sReportsV2.Resources.TextLanguage.Version</th>
                    <th>@sReportsV2.Resources.TextLanguage.DateTime</th>
                    <th>@sReportsV2.Resources.TextLanguage.State_</th>
                    <th></th>
                    <th>
                        <label class="form-checkbox-label form-checkbox-position">
                            <input id="selectAllCheckboxes" class="form-checkbox-field" type="checkbox" name="checkboxSelectAll" />
                            <i class="form-checkbox-button"></i>
                        </label>
                    </th>
                </tr>
            </thead>
            <tbody>

                @foreach (sReportsV2.DTOs.Form.DataOut.FormDataOut form in Model.Data)
                {
                    <tr>
                        <td onclick="loadFormInstances(event, '@form.Id', '@form.ThesaurusId','@form.Title','@form.Version.Id')">@form.Title</td>
                        <td onclick="loadFormInstances(event, '@form.Id', '@form.ThesaurusId','@form.Title','@form.Version.Id')">@form.Version.Major.@form.Version.Minor</td>
                        <td onclick="loadFormInstances(event, '@form.Id', '@form.ThesaurusId','@form.Title','@form.Version.Id')">@form.EntryDatetime</td>
                        <td onclick="loadFormInstances(event, '@form.Id', '@form.ThesaurusId','@form.Title','@form.Version.Id')">@form.State</td>
                        <td onclick="loadFormInstances(event, '@form.Id', '@form.ThesaurusId','@form.Title','@form.Version.Id')">
                            <i class="fas fa-folder folder-color"></i>
                        </td>
                        <td>
                            <label class="form-checkbox-label" id="checkBoxGroup">
                                <input value="@form.Id" class="form-checkbox-field" type="checkbox" data-title="@form.Title" name="checkboxDownload" />
                                <i class="form-checkbox-button"></i>
                            </label>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        @if (Model.Data.Count == 0)
        {
            <div class="no-result">No result!</div>
        }
        <div>
            <div class="pager-container">
                @Html.Partial("Pagination")
            </div>
        </div>
    </div>
