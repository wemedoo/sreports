<div id="conceptHeaderId" class="atoms-fix-head">
    <table class="table table-striped table-border" id="conceptTableId">
        <thead class="color-header">
            <tr class="table-header-row">
                <th class="unset-borders">UI</th>
                <th class="unset-borders">@sReportsV2.Resources.TextLanguage.RootSource.ToUpper()</th>
                <th class="unset-borders">@sReportsV2.Resources.TextLanguage.Name.ToUpper()</th>
                <th class="td-select unset-borders" style="padding-right: 63px">@sReportsV2.Resources.TextLanguage.Select_.ToUpper()</th>
            </tr>
        </thead>
        <tbody id="conceptsTableBody">
        </tbody>
    </table>
    <div id="loadMoreButtonContainer" class="load-more-button-container load-more-umls">
        <div class="load-more-button load-concepts" onclick="searchByTerm()">
            @sReportsV2.Resources.TextLanguage.LoadConcept
        </div>
    </div>
    <div class="atoms-no-result" id="conceptNoResult">
        <img class="margin-" src="~/Content/img/icons/no_results.svg">
        <br>
        <div class="no-result-text">@sReportsV2.Resources.TextLanguage.NoResultFound</div>
    </div>
</div>
