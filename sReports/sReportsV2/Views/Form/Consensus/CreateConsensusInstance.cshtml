@using sReportsV2.Common.Enums;
@using sReportsV2.DTOs.Form.DataOut;
@model FormDataOut

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="sotexsolutions.com">
    <title>Consensus - sReports by weMedoo</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous" />
    <link rel="stylesheet" href="~/Content/newMain.min.css">
    <link rel="stylesheet" href="~/Content/nestable.min.css">

    @Styles.Render("~/Content/css")

    @Scripts.Render("~/bundles/jquery")

</head>
<body class="consensus-body">
    <input type="hidden" id="formId" value="@Model.Id">
    <input type="hidden" id="consensusInstanceId" value="@ViewBag.ConsensusInstanceId">
    <input type="hidden" id="iterationId" value="@ViewBag.IterationId">
    <div class="consensus-visible">
        <div class="consensus-tabs-container">
            <div class="consensus-tab expand-button" onclick="showConsensusQuestionaire('consensus')">
                Consensus questionnaire
            </div>
            <div class="consensus-tab expand-button" onclick="showConsensusFormPreview('consensusFormPreview')">
                Form preview
            </div>
        </div>
        <div class="consensus consensus-tab-target consensus-questionaire" id="consensus">
            <div class="consensus-tree" id="consensusTree">
            </div>
        </div>
        <div class="consensus-form-preview consensus-tab-target consensus-questionaire" id="consensusFormPreview">

        </div>
    </div>
    <div class="button-wrapper-filter filter-button mt-2" id="submitFilterButton">
        <button class="btn btn-filter-primary w-100 h-100" type="button" onclick="submitConsensusInstance('@ViewBag.UserId','@ViewBag.ConsensusId', '@ViewBag.IsOutsideUser.ToString()')">
            @sReportsV2.Resources.TextLanguage.Submit
        </button>
    </div>


    @*<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>*@
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="~/Scripts/sReports/designer/consensus.js"></script>
    <script src="~/Scripts/sReports/designer/consensusInstance.js"></script>

    <script>
        $(document).ready(function () {
            loadConsensusInstanceTree();
        })
    </script>

</body>
</html>






